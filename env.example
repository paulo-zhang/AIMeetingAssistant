# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# Azure Speech Services
AZURE_SPEECH_KEY=your_azure_speech_key_here
AZURE_SPEECH_REGION=your_azure_region_here

# Anthropic Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ElevenLabs Voice Cloning
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Google Cloud Services
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id
GOOGLE_APPLICATION_CREDENTIALS=path_to_your_credentials.json

# Meeting Platform APIs
ZOOM_API_KEY=your_zoom_api_key_here
ZOOM_API_SECRET=your_zoom_api_secret_here
TEAMS_CLIENT_ID=your_teams_client_id_here
TEAMS_CLIENT_SECRET=your_teams_client_secret_here

# Database Configuration
REDIS_URL=redis://localhost:6379
POSTGRES_URL=postgresql://username:password@localhost:5432/meeting_assistant

# Application Settings
DEBUG=True
LOG_LEVEL=INFO
ENVIRONMENT=development

# Audio Settings
SAMPLE_RATE=16000
CHUNK_SIZE=1024
CHANNELS=1

# Meeting Settings
DEFAULT_ASSISTANT_ROLE=meeting_assistant
MAX_CONTEXT_LENGTH=50
MEETING_TIMEOUT=3600

# Security Settings
SECRET_KEY=your_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here
